{
  "name": "spotify-f2p",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "yarn build:tsc && yarn build:workspaces",
    "build:tsc": "tsc --build",
    "build:workspaces": "yarn workspaces foreach --exclude spotify-f2p run build",
    "ddb:admin": "DYNAMO_ENDPOINT=http://localhost:8000 dynamodb-admin",
    "deploy": "serverless deploy --stage prod",
    "dev": "concurrently 'yarn:dev:*'",
    "dev:sls": "serverless offline start",
    "dev:workspaces": "yarn workspaces foreach --exclude spotify-f2p run dev",
    "install:db": "sls dynamodb install",
    "start": "concurrently 'yarn:dev:*' 'yarn:watch' 'yarn:ddb:admin'",
    "watch": "tsc --build --watch"
  },
  "devDependencies": {
    "concurrently": "^7.1.0",
    "dynamodb-admin": "^4.5.0",
    "serverless": "^3.31.0",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-jetpack": "^0.11.1",
    "serverless-offline": "^8.7.0",
    "serverless-s3-deploy": "^0.10.1",
    "typescript": "^4.0.3"
  },
  "packageManager": "yarn@3.2.0"
}
