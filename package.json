{
  "name": "spotify-f2p",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "dev:sls": "serverless offline start",
    "dev:workspaces": "yarn workspaces foreach --exclude spotify-f2p run dev",
    "dev": "concurrently \"yarn:dev:*\"",
    "watch": "tsc --build --watch",
    "deploy": "serverless deploy --stage prod",
    "build:tsc": "tsc --build",
    "build:workspaces": "yarn workspaces foreach --exclude spotify-f2p run build",
    "build": "concurrently \"yarn:build:*\"",
    "lint": "tslint 'packages/**/*.{ts,tsx}'",
    "install:db": "sls dynamodb install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.1.0",
    "husky": "^4.3.0",
    "serverless": "3",
    "serverless-dynamodb-local": "^0.2.40",
    "serverless-iam-roles-per-function": "^3.2.0",
    "serverless-offline": "^8.7.0",
    "serverless-s3-sync": "^3.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "packageManager": "yarn@3.2.0"
}
